<% if @creators.present? %>
  $('#creators').append("<%= escape_javascript(render 'creators', object: [@creators, @card_number]) %>")
  $("#more_creator").replaceWith("<%= escape_javascript(
    link_to 'more', user_order_created_at_more_path({:user_id => @user.id, :number => @card_number}), :remote => true, :id => 'more_creator'
  ) %>");
<% else %>
  $("#more_creator").css('display','none');
<% end %>
